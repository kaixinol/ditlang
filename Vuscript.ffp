#endif
'Dim Argv1 as String Ptr=iif(D.place(0)=NULL Or D.place(1)=NULL,NULL,NEW  string(Mid(str_,*(D.place(0)).VINT,Len(str_)-*(D.place(1)).VINT+1))) 
