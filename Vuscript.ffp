   End If

   #if DBG
   DPrint(vbtab + "FF_WINMAIN()->varspace=" + Str(varspace))
   #endif
   If  FileLEN(ScriptPath) =0  Then

      printf "can't find file:" + ScriptPath + ",and this program will stop.(EMPTY?)"
      Exit_(2)
   End if
   'LoadScript(varspace)
